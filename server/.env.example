NODE_ENV=development

# LINE Login Channel (Web Login v2.1)
LINE_LOGIN_CHANNEL_ID=2008314464
LINE_LOGIN_CHANNEL_SECRET=your_channel_secret_here

# LINE Messaging API Channel
LINE_MESSAGING_CHANNEL_ACCESS_TOKEN=your_messaging_api_token_here
LINE_CHANNEL_SECRET=your_channel_secret_here

# JWT Secret (ランダムな文字列を使用 - 生成コマンド: openssl rand -base64 32)
JWT_SECRET=your_jwt_secret_here

# Server (ローカル開発用)
PORT=3000

# AWS (デプロイ時に使用)
AWS_REGION=ap-northeast-1
AWS_PROFILE=default
