{
  "servers": {
    "github-mcp": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp"
    },
    "fetch": {
        "command": "uvx",
        "args": [
          "mcp-server-fetch"
        ]
      },
      "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
  }
}